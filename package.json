{
  "name": "amirka",
  "version": "0.0.40-source",
  "description": "Minimalist Notation",
  "main": "./src/utils/index.js",
  "types": "./src/global.d.ts",
  "scripts": {
    "test": "rm -rf tmp/ && karma start karma.conf.js --single-run --browsers Chrome",
    "test:watch": "karma start karma.conf.js --browsers Chrome",
    "start": "webpack-dev-server --inline --config webpack.config.js --progress",
    "lint": "tslint -c tslint.json \"src/**/*.ts\" \"examples/**/*.ts\"",
    "build": "webpack --mode production",
    "setup": "rustup default nightly && rustup update nightly && rustup target add wasm32-unknown-unknown --toolchain nightly && cargo install --force --git https://github.com/alexcrichton/wasm-gc"
  },
  "keywords": [
    "CSS",
    "preprocessor",
    "parser",
    "minimalist",
    "notation",
    "modules",
    "util"
  ],
  "author": {
    "name": "Absolutely Amir",
    "email": "mr.amirka@ya.ru",
    "url": "https://github.com/mr-amirka"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mr-amirka/amirka.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "hard-source-webpack-plugin": "^0.12.0",
    "history": "^4.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "raw-loader": "^0.5.1",
    "react-frame-component": "^4.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rust-native-wasm-loader": "^0.8.1",
